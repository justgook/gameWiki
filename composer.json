{
    "require": {
        "php": ">=5.3.3",
        "slim/slim": "2.*",
        "slim/extras": "2.*",
        "slim/strong": "dev-master",
        "twig/twig"    : "1.*",
        "doctrine/orm": "2.3.3",
        "twitter/bootstrap": "dev-master",
        "littke/inspiritas-bootstrap": "dev-master"

    },
    "repositories": [
    {
        "type": "package",
        "package": {
            "version": "dev-master",
            "name": "twitter/bootstrap",
            "source": {
